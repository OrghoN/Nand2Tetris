/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a, out=Na);
    Not(in=b, out=Nb);
    Nand(a=a,b=Nb,out= c);
    Nand(a=b,b=Na, out = d);
    Nand(a=c, b=d, out = out);
}
